				"clear": true
			},
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": []
		},
		{
			"label": "Test eep_data_write and neo_driver_app",
			"type": "shell",
			"command": "${workspaceFolder}/scripts/ms-dev-sequence.ps1",
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": true,
				"panel": "shared",
				"showReuseMessage": true,
				"clear": true
			},
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": []
